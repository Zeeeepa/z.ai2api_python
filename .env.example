# API Authentication
AUTH_TOKEN=sk-your-api-key
SKIP_AUTH_TOKEN=false

# Optional: Multiple API keys file
# AUTH_TOKENS_FILE=tokens.txt

# LongCat Provider Token (required for LongCat models)
# LONGCAT_PASSPORT_TOKEN=your-longcat-token
# LONGCAT_TOKENS_FILE=longcat_tokens.txt

# Server Configuration
HOST=0.0.0.0
PORT=8080

# Logging
LOG_LEVEL=INFO

# Session Encryption (auto-generated if not set)
# SESSION_ENCRYPTION_KEY=your-32-byte-hex-key

# Optional: Custom session directory
# SESSION_DIR=.sessions

